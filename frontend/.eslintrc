{
	"root": true,
	"env": {
		"browser": true,
		"amd": true,
		"node": true,
		"es6": true,
		"jquery": true
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"parser": "babel-eslint"
	},
	"extends": [
		"plugin:vue/essential",
		"plugin:prettier/recommended",
		"eslint:recommended"
	],
	"globals": {
		"lofig": "writable",
		"grecaptcha": "readonly",
		"ga": "readonly",
		"moment": "readonly"
	},
	"rules": {
		"no-control-regex": 0,
		"no-var": 2
	}
}
