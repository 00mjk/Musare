{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "isolatedModules": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "jsx": "preserve",
    "types": [
      "vite/client",
      "@intlify/vite-plugin-vue-i18n/client",
      "vitest/globals"
    ]
  },
  "exclude": ["./src/index.html"]
}